
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<div ng-app="orderApp" ng-submit="login()" ng-controller="userController as uc">
    <form novalidate>
        First Name:<br>
        <input type="text" ng-model="firstName"><br>
        Last Name:<br>
        <input type="text" ng-model="lastName" />
        <br><br>
        <input type="submit" id="submit" value="Login" />
    </form>

</div>

